{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://p12pr.onrender.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "./build/index.html"
    }
  ]
}
